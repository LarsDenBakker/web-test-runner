{
  "name": "web-test-runner",
  "version": "0.0.20",
  "description": "",
  "main": "dist/core/runtime/web-test-runner.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc --build tsconfig-runtime.json",
    "build:watch": "rm -rf dist && tsc --watch & tsc --build tsconfig-runtime.json --watch",
    "test": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/**/*.test.js",
    "test:debug": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/**/*.test.js --debug",
    "test:fail": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/errors.test.js",
    "test:pass": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/c.test.js",
    "test:watch": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/**/*.test.js --watch",
    "test:404-import": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/404-import.test.js",
    "test:playwright": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/**/*.js --config demo/playwright.config.js",
    "test:browserstack": "export LOCAL_TESTING=true && node dist/core/cli.js demo/test/**/*.js --config demo/browserstack.config.js",
    "test:performance": "export LOCAL_TESTING=true && node dist/core/cli.js demo/performance/**/*.test.js ",
    "release": "npm publish",
    "start": "es-dev-server --open demo/ --node-resolve"
  },
  "files": [
    "assets",
    "dist"
  ],
  "bin": {
    "wtr": "./dist/core/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarsDenBakker/web-test-runner.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LarsDenBakker/web-test-runner/issues"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "homepage": "https://github.com/LarsDenBakker/web-test-runner#readme",
  "dependencies": {
    "@types/deepmerge": "^2.2.0",
    "@types/diff": "^4.0.2",
    "@types/koa": "^2.11.3",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/uuid": "^7.0.3",
    "browserstack-local": "^1.4.5",
    "chalk": "^4.0.0",
    "co-body": "^6.0.0",
    "command-line-args": "^5.1.1",
    "deepmerge": "^4.2.2",
    "diff": "^4.0.2",
    "es-dev-server": "^1.50.5",
    "globby": "^11.0.0",
    "koa": "^2.11.0",
    "log-update": "^4.0.0",
    "mocha": "^7.1.2",
    "mochawesome": "^6.1.1",
    "playwright": "^1.0.2",
    "puppeteer": "^3.0.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "tap-spec": "^5.0.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@bundled-es-modules/chai": "^4.2.2",
    "@types/co-body": "^5.1.0",
    "@types/command-line-args": "^5.0.0",
    "@types/globby": "^9.1.0",
    "@types/mocha": "^7.0.2",
    "@types/puppeteer": "^2.1.0",
    "lodash-es": "^4.17.15",
    "typescript": "^3.8.3"
  }
}
